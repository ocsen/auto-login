{
    "manifest_version": 2,
    "name": "小美后台自动登陆",
    "version": "0.0.1",
    "icons":{"16":"icon_16.png","48":"icon_48.png","128":"icon_128.png"},
    "background": {
        "scripts": ["jquery-2.0.0.min.js", "background.js"]
    },
    "browser_action": {
        "default_icon": {
            "19": "icon.png",
            "38": "icon.png",
            "default_popup": "popup.html"
        },
        "default_title": "自动登陆",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "cookies",
        "notifications",
        "windows",
        "http://*/",
        "https://*/"
    ],
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "js": ["jquery-2.0.0.min.js", "content_script.js"]
    }]
}
